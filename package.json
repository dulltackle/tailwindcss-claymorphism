{
  "name": "tailwindcss-claymorphism",
  "version": "0.11.1",
  "description": "Tailwind CSS plugin to generate claymorphism style utilities",
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "tailwindcss",
    "claymorphism",
    "claymorphic",
    "plugin",
    "style",
    "css"
  ],
  "repository": "https://github.com/dulltackle/tailwindcss-claymorphism.git",
  "bugs": "https://github.com/dulltackle/tailwindcss-claymorphism/issues",
  "homepage": "https://github.com/dulltackle/tailwindcss-claymorphism#readme",
  "author": {
    "name": "dulltackle",
    "email": "dulltackle@outlook.com"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "lint": "eslint --quiet src/*",
    "typecheck": "tsc",
    "format": "prettier --write --ignore-unknown src/*",
    "dev:test": "vitest",
    "dev:e2e": "pnpm run build && vite --config test/e2e/vite.config.e2e.ts",
    "dev:e2e-build": "pnpm run build && vite build --config test/e2e/vite.config.e2e.ts"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest-matcher-utils": "^28.1.3",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.4.14",
    "prettier": "3.0.2",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.5.4",
    "vite": "^5.0.0",
    "vitest": "^0.18.0"
  },
  "peerDependencies": {
    "tailwindcss": "^3.1.6"
  }
}
